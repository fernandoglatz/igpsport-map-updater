<?xml version="1.0"?>
<translations>
    <translation>
        <name>highway motorway -&gt; trunk</name>
        <description>Map unsupported highway=motorway to highway=trunk (device-supported).</description>
        <match type="way">
            <tag k="highway" v="motorway"/>
        </match>
        <output>
            <copy-unmatched/>
            <tag k="highway" v="trunk"/>
        </output>
    </translation>

